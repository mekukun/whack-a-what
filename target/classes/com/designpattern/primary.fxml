<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="200.0" prefWidth="503.0" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.designpattern.PrimaryController">
   <children>
      <Label alignment="CENTER" text="Whack-a-what?" textAlignment="CENTER">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <VBox alignment="CENTER" fillWidth="false" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label alignment="CENTER" text="Name" textAlignment="CENTER">
               <font>
                  <Font size="14.0" />
               </font></Label>
            <TextField alignment="CENTER" prefHeight="25.0" prefWidth="398.0">
               <VBox.margin>
                  <Insets />
               </VBox.margin></TextField>
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label alignment="CENTER" text="Choose your mole" textAlignment="CENTER">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <ComboBox fx:id="moledp" prefWidth="150.0" />
         </children>
      </VBox>
      <Button fx:id="primaryButton" onAction="#switchToSecondary" text="Start" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
